{
  "repository": "git@github.com:STDSuperman/super-image-cropper.git",
  "author": "STDSuperman <2750556766@qq.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "commit": "git add -A && git-cz",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts",
    "format:fix": "prettier --write **/*.ts",
    "husky-pre-commit": "husky set .husky/_/pre-commit 'npm run test'",
    "husky-commit-msg": "husky set .husky/_/commit-msg 'npm run commitlint --edit $1'",
    "clean": "rimraf dist",
    "build": "turbo run build",
    "format": "prettier --check **/*.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "tsx ./scripts/release"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chalk": "^5.1.2",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "enquirer": "^2.3.6",
    "esbuild": "^0.14.23",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.2",
    "minimist": "^1.2.7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8",
    "tslib": "^2.3.1",
    "tsx": "^3.12.1",
    "turbo": "^1.6.3",
    "type-fest": "^3.2.0",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
